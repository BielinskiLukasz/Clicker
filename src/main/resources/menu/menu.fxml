<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import java.net.URL?>
<GridPane
        alignment="center"
        hgap="10"
        maxHeight="275.0" maxWidth="450.0"
        minHeight="275.0" minWidth="450.0"
        prefHeight="275.0" prefWidth="450.0"
        vgap="10"
        xmlns="http://javafx.com/javafx/8.0.141"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="menu.Menu">

    <columnConstraints>
        <ColumnConstraints
                halignment="CENTER"/>
    </columnConstraints>

    <Label
            text="PJATK Builder Simulator 2018"
            prefHeight="50.0" prefWidth="300.0"
            GridPane.columnIndex="0"
            GridPane.rowIndex="1"/>

    <Button
            fx:id="startButton"
            onAction="#startGame"
            GridPane.columnIndex="0"
            GridPane.rowIndex="2"
            text="Start"/>

    <stylesheets>
        <URL
                value="@style.css"/>
    </stylesheets>

</GridPane>
